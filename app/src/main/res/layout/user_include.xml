<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
                style="@style/baseLayout"
                android:id="@+id/user_include_layout">

    <TextView
            android:id="@+id/usercomu_item_alias_txt"
            style="@style/textBase.textFieldLabel1"
            android:layout_alignParentStart="true"/>

    <TextView
            android:id="@+id/usercomu_item_username_txt"
            style="@style/textBase.textFieldLabel1"
            android:layout_marginTop="0dp"
            android:layout_toEndOf="@id/usercomu_item_alias_txt"
            android:layout_alignTop="@id/usercomu_item_alias_txt"
            android:layout_alignBottom="@id/usercomu_item_alias_txt"/>

</RelativeLayout>