<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
                android:id="@+id/comunidad_list_item_view"
                style="@style/baseLayout">

    <TextView
            android:id="@+id/nombreComunidad_view"
            style="@style/textBase.textFieldLabel1"/>

    <TextView
            android:id="@+id/municipio_view"
            style="@style/textBase.textFieldLabel2"
            android:layout_alignParentStart="true"
            android:layout_below="@id/nombreComunidad_view"
            />

    <TextView
            android:id="@+id/provincia_view"
            style="@style/textBase.textFieldLabel2"
            android:layout_alignParentStart="true"
            android:layout_below="@id/municipio_view"
            />

</RelativeLayout>