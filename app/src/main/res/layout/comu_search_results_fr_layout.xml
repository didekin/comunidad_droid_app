<?xml version="1.0" encoding="utf-8"?>
<LinearLayout style="@style/baseLayout.withPadding"
              xmlns:android="http://schemas.android.com/apk/res/android"
              xmlns:tools="http://schemas.android.com/tools"
              android:id="@+id/comu_list_frg_layout"
              tools:context="com.didekindroid.com.didekinaar.comunidad.ComuSearchResultsListFr"
              >
    <ListView
        android:id="@id/android:list"
        style="@style/basicList"
        android:choiceMode="singleChoice"
        android:listSelector="@color/transparent">
    </ListView>
    <TextView
        android:id="@id/android:empty"
        style="@style/textBase.no_item_list_message"
        android:text="@string/comu_list_no_result_txt"
        android:visibility="gone">
    </TextView>
</LinearLayout>